{
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.582.0",
    "@aws-sdk/lib-dynamodb": "^3.582.0",
    "@aws-sdk/util-dynamodb": "^3.529.1",
    "aws-sdk": "^2.1599.0",
    "http": "^0.0.1-security"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-when": "^3.6.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "name": "ng-customer",
  "version": "1.0.0",
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "jest --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "modulePaths": [
      "<rootDir>"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/src/",
      "<rootDir>/node_modules/"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": "<rootDir>/unit_tests/",
          "outputName": "unitTestReport.xml"
        }
      ]
    ]
  }
}
